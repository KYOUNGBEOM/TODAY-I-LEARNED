#include <iostream>
#include <algorithm>
using namespace std;

// 시간은 이상, 미만으로 생각

// 내 풀이
int A, B, C;
int a[100];
int arrive1, leave1;
int arrive2, leave2;
int arrive3, leave3;
int t1[100], t2[100], t3[100];
int truck[100];
int result1, result2, result3;
int sum = 0;

int main() {
	cin >> A >> B >> C;
	cin >> arrive1 >> leave1;
	cin >> arrive2 >> leave2;
	cin >> arrive3 >> leave3;

	for (int i = arrive1-1; i < leave1-1; i++) {
		t1[i]++;
	}
	for (int i = arrive2-1; i < leave2-1; i++) {
		t2[i]++;
	}
	for (int i = arrive3-1; i < leave3-1; i++) {
		t3[i]++;
	}

	for (int i = 0; i < 100; i++) {
		truck[i] = t1[i] + t2[i] + t3[i];

			if (truck[i] == 3) {
				result1++;
			}

			else if (truck[i] == 2) {
				result2++;
			}

			else if (truck[i] == 1) {
				result3++;
			}
	}


	sum = sum + 3 * (result1 * C);
	sum = sum + 2 * (result2 * B);
	sum = sum + (result3 * A);
	
	cout << sum;

	return 0;
}

#include <iostream>
#include <algorithm>
using namespace std;

// 강 풀이
int A, B, C, a, b, cnt[104], ret;

int main() {
	cin >> A >> B >> C;
	for (int i = 0; i < 3; i++) {
		cin >> a >> b;
		for (int j = a; j < b; j++)cnt[j]++;
	}
	for (int j = 1; j < 100; j++) {
		if (cnt[j]) {
			if (cnt[j] == 1) ret += A;
			else if (cnt[j] == 2) ret += B * 2;
			else if (cnt[j] == 3) ret += C * 3;
		}
	}
	cout << ret;

	return 0;
}
