// 내 코드 -> 어김없이 시간초과.. 얼른 시간복잡도 공부를 해야겠다..
#include <iostream>
#include <algorithm>
#include <string>

using namespace std;

int n;
int f = 10;
string s;

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);
	while(cin >> n) {
		long long i = 1, t = 1;
		while (true) {
			if (i % n == 0) {
				s = to_string(i);
				cout << s.size() << "\n";
				break;
			}
			else {
				t *= f;
				i += t;
			}
		}
	}


	return 0;
}
// 강의 코드
